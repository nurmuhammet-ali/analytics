{
    "name": "nurmuhammet.ali/analytics",
    "description": "AndreasElia/laravel-analytics alternative, but with IP column",
    "keywords": ["laravel", "analytics", "middleware", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nurmuhammet Allanov",
            "email": "nurmuhammet@mail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-intl": "*",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/collections": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/view": "^8.0",
        "jenssegers/agent": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Nurmuhammet\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nurmuhammet\\Analytics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nurmuhammet\\Analytics\\AnalyticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.5"
    }
}
